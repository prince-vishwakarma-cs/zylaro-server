{
    "version": 2,
    "builds": [
      {
        "src": "dist/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/app.js"
      }
    ],
    "outputDirectory": "dist",
    "functions": {
      "api/**.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }
  